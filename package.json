{
  "name": "securian-exercise",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "nyc wdio run wdio.conf.js",
    "allure:generate": "npx allure generate allure-results --clean -o allure-report",
    "allure:open": "npx allure open allure-report",
    "allure:serve": "npm run allure:generate && npm run allure:open",
    "start": "npm test"
  },
  "nyc": {
  "include": [
    "src/pages/*.js"
  ],
  "reporter": [
    "text",
    "html"
  ]
},
  "type": "module",
  "keywords": [],
  "author": "Subhrakanta Sahoo",
  "license": "ISC",
  "description": "WebdriverIO project using Cucumber framework",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.12.6",
    "@wdio/cli": "^8.44.1",
    "@wdio/cucumber-framework": "^9.12.6",
    "@wdio/local-runner": "^9.12.6",
    "@wdio/logger": "^9.4.4",
    "@wdio/selenium-standalone-service": "^8.14.0",
    "@wdio/spec-reporter": "^9.12.6",
    "allure-commandline": "^2.33.0",
    "chromedriver": "^137.0.4",
    "cucumber": "^6.0.7",
    "eslint": "^9.25.1",
    "image-size": "^2.0.2",
    "nyc": "^17.1.0",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-wait-for": "^3.1.0"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "wdio": "^6.0.1",
    "webdriverio": "^8.44.1",
    "winston": "^3.17.0"
  }
}
